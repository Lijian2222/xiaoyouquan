.my-school-page{min-height:100vh;background-color:#f7f7f7;font-family:PingFang SC,Microsoft YaHei,sans-serif;padding-bottom:5.33vw}.status-bar{display:flex;justify-content:space-between;align-items:center;padding:2.67vw 4vw}.status-bar .time{font-size:4.27vw;font-weight:700}.status-bar .status-bar-center{width:36vw;height:5.33vw;background-color:#000;border-radius:4vw}.status-bar .status-bar-right{display:flex;align-items:center}.status-bar .status-bar-right .signal,.status-bar .status-bar-right .wifi,.status-bar .status-bar-right .battery{margin-left:1.33vw;font-size:3.73vw}.status-bar .status-bar-right .signal:before{content:"\2582\2583\2584"}.status-bar .status-bar-right .wifi:before{content:"Wi-Fi"}.status-bar .status-bar-right .battery:before{content:"\2590"}.page-header{display:flex;justify-content:space-between;align-items:center;padding:2.67vw 4vw;position:relative}.page-header .back-button{font-size:5.33vw;padding:1.33vw}.page-header .page-title{font-size:4.8vw;font-weight:700;position:absolute;left:50%;transform:translate(-50%)}.page-header .header-right{display:flex;align-items:center}.page-header .header-right .dots{font-size:4vw;margin-right:2.67vw}.page-header .header-right .circle-icon{width:6.67vw;height:6.67vw;border-radius:50%;background-color:#f0f0f0;display:flex;align-items:center;justify-content:center;font-size:4vw}.education-tabs{display:flex;justify-content:space-between;padding:2.67vw 4vw;margin-bottom:5.33vw}.education-tabs .education-tab{width:29.33vw;height:29.33vw;background-color:#fff;border-radius:4vw;display:flex;flex-direction:column;align-items:center;justify-content:center;box-shadow:0 .53vw 2.67vw rgba(0,0,0,.05)}.education-tabs .education-tab.active:after{content:"";position:absolute;bottom:-5.33vw;left:50%;transform:translate(-50%);width:0;height:0;border-left:2.67vw solid transparent;border-right:2.67vw solid transparent;border-top:2.67vw solid #fff}.education-tabs .education-tab .education-icon{width:10.67vw;height:10.67vw;margin-bottom:2.67vw}.education-tabs .education-tab .education-text{font-size:3.73vw;color:#999}.education-tabs .education-tab .education-text.active-text{color:#0086ff}.school-card{margin:0 4vw;background-color:#fff;border-radius:4vw;padding:5.33vw;box-shadow:0 .53vw 2.67vw rgba(0,0,0,.05)}.school-card .degree-item{margin-bottom:8vw}.school-card .degree-item .degree-tag{display:flex;align-items:center;background-color:#0086ff;border-radius:5.33vw;padding:1.33vw 4vw;width:-webkit-fit-content;width:fit-content;margin-bottom:2.67vw}.school-card .degree-item .degree-tag .degree-dot{width:2.13vw;height:2.13vw;background-color:#fff;border-radius:50%;margin-right:1.33vw}.school-card .degree-item .degree-tag .degree-text{color:#fff;font-size:3.73vw}.school-card .degree-item .school-info{padding:2.67vw}.school-card .degree-item .school-info .school-title{font-size:3.73vw;color:#333;margin-bottom:4vw}.school-card .degree-item .school-info .school-logo-container{display:flex;justify-content:center;margin-bottom:2.67vw}.school-card .degree-item .school-info .school-logo-container .school-logo{width:21.33vw;height:21.33vw}.school-card .degree-item .school-info .school-name-container{text-align:center;margin-bottom:4vw}.school-card .degree-item .school-info .school-name-container .school-name{font-size:5.33vw;color:#333;font-weight:700;display:block;margin-bottom:1.33vw}.school-card .degree-item .school-info .school-name-container .school-name-en{font-size:3.2vw;color:#666;display:block}.school-card .degree-item .school-info .school-description .description-text{font-size:3.2vw;color:#999;line-height:4.8vw}.school-card .view-details-btn{width:40vw;height:10.67vw;background-color:#0086ff;border-radius:5.33vw;display:flex;align-items:center;justify-content:center;margin:5.33vw auto 2.67vw}.school-card .view-details-btn .btn-text{color:#fff;font-size:3.73vw}
